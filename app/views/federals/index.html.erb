<h2>Diputados Federales</h2>

<table class="table table-striped">
  <tr>
    <th>Nombre</th>
    <th>Acciones</th>
  </tr>
  <% @federals.each do |federal| %>
    <tr>
      <td><%= federal.first_name %> <%= federal.last_name %></td>
      <td><%= link_to 'Editar', edit_federal_path(federal.id) %> |
        <%= link_to 'Borrar', federal_path(federal), method: :delete, data: { confirm: 'Estas seguro?' } %></td>
    </tr>
  <% end %>
</table>
